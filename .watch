{
  "build": [{
    "patterns": ["src/**/*.js"],
    "command": "npm run build:js -- --only",
    "appendFiles": true
  }, {
    "patterns": ["src/**/*.js"],
    "command": "npm run build:flow"
  }, {
    "patterns": ["src/**/*"],
    "command": "npm run clean && npm run build",
    "change": false, "add": false, "delete": true
  }],
  "test": [{
    "patterns": ["src/**/*.js", ".eslintrc"],
    "command": "npm run lint -s"
  }]
}
